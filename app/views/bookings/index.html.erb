<div class="cards">

  <% @booking_list.each do |booking| %>
    <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path booking.rocket.photo.key, height: 300, crop: :fill %>')">
      <%= booking.rocket.name %>, <%= booking.start_date %>, <%= booking.end_date %>, <%= booking.status %>, <%= booking.price %>
    </div>
  <% end %>

</div>
